<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/area_psico"
    android:background="@android:color/holo_blue_light"
    android:visibility="gone">
    <android.support.v7.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_margin="8dp">

        <TextView
            android:id="@+id/opcion_psico1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:text="OPCION 1 "
            android:textSize="12dp"
            android:textColor="@android:color/white"/>

        <RadioGroup
            android:id="@+id/group_psico1"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:checkedButton="@+id/p11"
            android:layout_weight="1"
            >
            <android.support.v7.widget.AppCompatRadioButton
                android:id="@+id/p11"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="0"
                android:layout_weight="1"
                android:buttonTint="@android:color/white"
                android:textColor="@android:color/white"
                app:buttonTint="@color/colorWhite"/>
            <android.support.v7.widget.AppCompatRadioButton
                android:id="@+id/p12"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="1"
                android:layout_weight="1"
                android:buttonTint="@android:color/white"
                android:textColor="@android:color/white"
                app:buttonTint="@color/colorWhite"/>
            <android.support.v7.widget.AppCompatRadioButton
                android:id="@+id/p13"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="2"
                android:layout_weight="1"
                android:buttonTint="@android:color/white"
                android:textColor="@android:color/white"
                app:buttonTint="@color/colorWhite"/>
            <android.support.v7.widget.AppCompatRadioButton
                android:id="@+id/p14"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="3"
                android:layout_weight="1"
                android:buttonTint="@android:color/white"
                android:textColor="@android:color/white"
                app:buttonTint="@color/colorWhite"/>

        </RadioGroup>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_margin="8dp">

        <TextView
            android:id="@+id/opcion_psico2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:text="OPCION 2 "
            android:textSize="12dp"
            android:textColor="@android:color/white"
            />

        <RadioGroup
            android:id="@+id/group_psico2"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:checkedButton="@+id/p21"
            android:layout_weight="1"
            >
            <android.support.v7.widget.AppCompatRadioButton
                android:id="@+id/p21"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="0"
                android:layout_weight="1"
                android:buttonTint="@android:color/white"
                android:textColor="@android:color/white"
                app:buttonTint="@color/colorWhite"/>
            <android.support.v7.widget.AppCompatRadioButton
                android:id="@+id/p22"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="1"
                android:layout_weight="1"
                android:buttonTint="@android:color/white"
                android:textColor="@android:color/white"
                app:buttonTint="@color/colorWhite"/>
            <android.support.v7.widget.AppCompatRadioButton
                android:id="@+id/p23"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="2"
                android:layout_weight="1"
                android:buttonTint="@android:color/white"
                android:textColor="@android:color/white"
                app:buttonTint="@color/colorWhite"/>
            <android.support.v7.widget.AppCompatRadioButton
                android:id="@+id/p24"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="3"
                android:layout_weight="1"
                android:buttonTint="@android:color/white"
                android:textColor="@android:color/white"
                app:buttonTint="@color/colorWhite"/>

        </RadioGroup>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_margin="8dp">

        <TextView
            android:id="@+id/opcion_psico3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:text="OPCION 3 "
            android:textSize="12dp"
            android:textColor="@android:color/white"/>

        <RadioGroup
            android:id="@+id/group_psico3"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:checkedButton="@+id/p31"
            android:layout_weight="1"
            >
            <android.support.v7.widget.AppCompatRadioButton
                android:id="@+id/p31"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="0"
                android:layout_weight="1"
                android:buttonTint="@android:color/white"
                android:textColor="@android:color/white"
                app:buttonTint="@color/colorWhite"/>
            <android.support.v7.widget.AppCompatRadioButton
                android:id="@+id/p32"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="1"
                android:layout_weight="1"
                android:buttonTint="@android:color/white"
                android:textColor="@android:color/white"
                app:buttonTint="@color/colorWhite"/>
            <android.support.v7.widget.AppCompatRadioButton
                android:id="@+id/p33"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="2"
                android:layout_weight="1"
                android:buttonTint="@android:color/white"
                android:textColor="@android:color/white"
                app:buttonTint="@color/colorWhite"/>
            <android.support.v7.widget.AppCompatRadioButton
                android:id="@+id/p34"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="3"
                android:layout_weight="1"
                android:buttonTint="@android:color/white"
                android:textColor="@android:color/white"
                app:buttonTint="@color/colorWhite"/>
        </RadioGroup>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_margin="8dp">

        <TextView
            android:id="@+id/opcion_psico4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:text="OPCION 4 "
            android:textSize="12dp"
            android:textColor="@android:color/white"/>

        <RadioGroup
            android:id="@+id/group_psico4"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:checkedButton="@+id/p41"
            android:layout_weight="1"
            >
            <android.support.v7.widget.AppCompatRadioButton
                android:id="@+id/p41"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="0"
                android:layout_weight="1"
                android:buttonTint="@android:color/white"
                android:textColor="@android:color/white"
                app:buttonTint="@color/colorWhite"/>
            <android.support.v7.widget.AppCompatRadioButton
                android:id="@+id/p42"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="1"
                android:layout_weight="1"
                android:buttonTint="@android:color/white"
                android:textColor="@android:color/white"
                app:buttonTint="@color/colorWhite"/>
            <android.support.v7.widget.AppCompatRadioButton
                android:id="@+id/p43"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="2"
                android:layout_weight="1"
                android:buttonTint="@android:color/white"
                android:textColor="@android:color/white"
                app:buttonTint="@color/colorWhite"/>
            <android.support.v7.widget.AppCompatRadioButton
                android:id="@+id/p44"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="3"
                android:layout_weight="1"
                android:buttonTint="@android:color/white"
                android:textColor="@android:color/white"
                app:buttonTint="@color/colorWhite"/>
        </RadioGroup>

    </LinearLayout>

    </android.support.v7.widget.LinearLayoutCompat>
</LinearLayout>