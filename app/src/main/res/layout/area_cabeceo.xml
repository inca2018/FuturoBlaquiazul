<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/area_tactica4"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/holo_blue_light"
    android:orientation="vertical"
    android:visibility="gone">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="8dp"
                android:text="Cabeceo:"
                android:textAlignment="center"
                android:textColor="@color/blanco"
                android:textSize="@dimen/letra_fisico"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/op1234"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="4"
                    android:gravity="center"
                    android:text="Nº 1 "
                    android:textColor="@android:color/white"
                    android:textSize="@dimen/letra_fisico" />

                <RadioGroup
                    android:id="@+id/grupo_tecnico1234"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:checkedButton="@+id/cab112"
                    android:orientation="horizontal">

                    <android.support.v7.widget.AppCompatRadioButton
                        android:id="@+id/cab112"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:buttonTint="@android:color/white"
                        android:text="0"
                        android:textColor="@android:color/white"
                        app:buttonTint="@color/colorWhite" />

                    <android.support.v7.widget.AppCompatRadioButton
                        android:id="@+id/cab122"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:buttonTint="@android:color/white"
                        android:text="1"
                        android:textColor="@android:color/white"
                        app:buttonTint="@color/colorWhite" />

                    <android.support.v7.widget.AppCompatRadioButton
                        android:id="@+id/cab132"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:buttonTint="@android:color/white"
                        android:text="2"
                        android:textColor="@android:color/white"
                        app:buttonTint="@color/colorWhite" />

                    <android.support.v7.widget.AppCompatRadioButton
                        android:id="@+id/cab142"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:buttonTint="@android:color/white"
                        android:text="3"
                        android:textColor="@android:color/white"
                        app:buttonTint="@color/colorWhite" />

                </RadioGroup>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/op2234"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="4"
                    android:gravity="center"
                    android:text="Nº 2"
                    android:textColor="@android:color/white"
                    android:textSize="@dimen/letra_fisico" />

                <RadioGroup
                    android:id="@+id/grupo_tecnico2234"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:checkedButton="@+id/cab212"
                    android:orientation="horizontal">

                    <android.support.v7.widget.AppCompatRadioButton
                        android:id="@+id/cab212"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:buttonTint="@android:color/white"
                        android:text="0"
                        android:textColor="@android:color/white"
                        app:buttonTint="@color/colorWhite" />

                    <android.support.v7.widget.AppCompatRadioButton
                        android:id="@+id/cab222"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:buttonTint="@android:color/white"
                        android:text="1"
                        android:textColor="@android:color/white"
                        app:buttonTint="@color/colorWhite" />

                    <android.support.v7.widget.AppCompatRadioButton
                        android:id="@+id/cab232"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:buttonTint="@android:color/white"
                        android:text="2"
                        android:textColor="@android:color/white"
                        app:buttonTint="@color/colorWhite" />

                    <android.support.v7.widget.AppCompatRadioButton
                        android:id="@+id/cab242"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:buttonTint="@android:color/white"
                        android:text="3"
                        android:textColor="@android:color/white"
                        app:buttonTint="@color/colorWhite" />

                </RadioGroup>

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/op3234"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="4"
                    android:gravity="center"
                    android:text="Nº 3"
                    android:textColor="@android:color/white"
                    android:textSize="@dimen/letra_fisico" />

                <RadioGroup
                    android:id="@+id/grupo_tecnico3234"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:checkedButton="@+id/cab312"
                    android:orientation="horizontal">

                    <android.support.v7.widget.AppCompatRadioButton
                        android:id="@+id/cab312"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:buttonTint="@android:color/white"
                        android:text="0"
                        android:textColor="@android:color/white"
                        app:buttonTint="@color/colorWhite" />

                    <android.support.v7.widget.AppCompatRadioButton
                        android:id="@+id/cab322"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:buttonTint="@android:color/white"
                        android:text="1"
                        android:textColor="@android:color/white"
                        app:buttonTint="@color/colorWhite" />

                    <android.support.v7.widget.AppCompatRadioButton
                        android:id="@+id/cab332"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:buttonTint="@android:color/white"
                        android:text="2"
                        android:textColor="@android:color/white"
                        app:buttonTint="@color/colorWhite" />

                    <android.support.v7.widget.AppCompatRadioButton
                        android:id="@+id/cab342"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:buttonTint="@android:color/white"
                        android:text="3"
                        android:textColor="@android:color/white"
                        app:buttonTint="@color/colorWhite" />
                </RadioGroup>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/op4234"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="4"
                    android:gravity="center"
                    android:text="Nº 4"
                    android:textColor="@android:color/white"
                    android:textSize="@dimen/letra_fisico" />

                <RadioGroup
                    android:id="@+id/grupo_tecnico4234"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:checkedButton="@+id/cab412"
                    android:orientation="horizontal">

                    <android.support.v7.widget.AppCompatRadioButton
                        android:id="@+id/cab412"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:buttonTint="@android:color/white"
                        android:text="0"
                        android:textColor="@android:color/white"
                        app:buttonTint="@color/colorWhite" />

                    <android.support.v7.widget.AppCompatRadioButton
                        android:id="@+id/cab422"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:buttonTint="@android:color/white"
                        android:text="1"
                        android:textColor="@android:color/white"
                        app:buttonTint="@color/colorWhite" />

                    <android.support.v7.widget.AppCompatRadioButton
                        android:id="@+id/cab432"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:buttonTint="@android:color/white"
                        android:text="2"
                        android:textColor="@android:color/white"
                        app:buttonTint="@color/colorWhite" />

                    <android.support.v7.widget.AppCompatRadioButton
                        android:id="@+id/cab442"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:buttonTint="@android:color/white"
                        android:text="3"
                        android:textColor="@android:color/white"
                        app:buttonTint="@color/colorWhite" />
                </RadioGroup>

            </LinearLayout>

        </LinearLayout>


    </LinearLayout>
</LinearLayout>